<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IAdContract" maxReceivedMessageSize="25000000" textEncoding="utf-8" transferMode="Streamed" messageEncoding="Text">
          <readerQuotas maxArrayLength="5000000"/>
        </binding>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8000/Service.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAdContract" contract="ServiceReference.IAdContract" name="BasicHttpBinding_IAdContract"/>
    </client>
  </system.serviceModel>
  <connectionStrings>
    <add name="PrisonConnection" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Prison;Integrated Security=True"/>

  </connectionStrings>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
