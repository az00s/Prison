@model IEnumerable<PlaceOfStayViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Места содержания задержанных</h2>

<p>
    @Html.ActionLink("Добавить место", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>

        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new {id=item.PlaceID }) |
            @Html.ActionLink("Удалить", "Delete", new { id = item.PlaceID })
        </td>
    </tr>
}

</table>
